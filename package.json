{
  "name": "nautico",
  "version": "1.0.3",
  "description": "Cloudflare Pages Fullstack Setup With Hono Backend",
  "author": "Jose J. Cruz",
  "type": "module",
  "keywords": [
    "cloudflare",
    "pages",
    "hono"
  ],
  "engines": {
    "node": ">=20.12.0"
  },
  "imports": {
    "#backend/*": "./backend/*"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "deploy:wrangler": "npm run build && wrangler pages deploy ./dist",
    "preview:wrangler": "npm run build && wrangler pages dev ./dist",
    "lint": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "generate-types": "wrangler types",
    "type-check": "npm run generate-types && tsc"
  },
  "dependencies": {
    "@apollo/client": "^3.13.5",
    "@hookform/resolvers": "^5.0.0",
    "@radix-ui/react-aspect-ratio": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-select": "^2.1.7",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.7",
    "@tailwindcss/vite": "^4.0.15",
    "bootstrap": "^5.3.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "graphql": "^16.10.0",
    "graphql-yoga": "^5.13.2",
    "hono": "^4.4.0",
    "lucide-react": "^0.483.0",
    "luxon": "^3.6.0",
    "next-themes": "^0.4.6",
    "react": "^18.0.0",
    "react-bootstrap": "^2.10.9",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.55.0",
    "react-icons": "^5.5.0",
    "react-photo-album": "^3.1.0",
    "react-router-dom": "^6.10.0",
    "sonner": "^2.0.3",
    "reactstrap": "^9.2.3",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.15",
    "tw-animate-css": "^1.2.4",
    "yet-another-react-lightbox": "^3.22.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250320.0",
    "@eslint/js": "^9.21.0",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.13.11",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.8",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "wrangler": "^4.4.0"
  }
}
